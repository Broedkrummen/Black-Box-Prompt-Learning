    "/seo/analyze": {
      "get": {
        "tags": [
          "On Page SEO"
        ],
        "summary": "On Page Analysis",
        "description": "Perform a comprehensive on-page SEO analysis of a website including meta tags, headings, images, links, and structural issues.",
        "parameters": [
          {
            "name": "website",
            "in": "query",
            "description": "Website domain or full URL to analyze",
            "required": true,
            "schema": {
              "type": "string",
              "example": "codeconia.com"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful on-page analysis",
            "content": {
              "application/json": {
                "example": {
                  "basic": {
                    "websiteurl": "codeconia.com",
                    "title": "Codeconia -Learn Programming",
                    "favicon": "http://www.google.com/s2/favicons?domain=codeconia.com"
                  },
                  "webtitle": {
                    "title": "Codeconia -Learn Programming",
                    "length": 28,
                    "suggestion": "Title length is good."
                  },
                  "metadescription": {
                    "description": "Programmer Guide",
                    "length": 16,
                    "suggestion": "Meta description is too short. Aim for 120-160 characters."
                  },
                  "metakeywords": {
                    "keywords": null,
                    "counts": 2,
                    "suggestion": "Meta keywords not found (optional in modern SEO)."
                  },
                  "headings": {
                    "h1": {
                      "count": 1
                    },
                    "h2": {
                      "count": 0
                    },
                    "h3": {
                      "count": 0
                    },
                    "h4": {
                      "count": 0
                    },
                    "h5": {
                      "count": 25
                    },
                    "h6": {
                      "count": 0
                    },
                    "suggestion": [
                      "No H2 headings found. Add sub-headings for structure."
                    ]
                  },
                  "sitemap_robots": [
                    "robots.txt",
                    "sitemap.xml"
                  ],
                  "iframe": {
                    "count": 0,
                    "suggestion": "No iframe usage. Good!"
