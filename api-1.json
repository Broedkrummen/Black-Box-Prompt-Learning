{
  "openapi": "3.1.0",
  "info": {
    "title": "VebAPI - SEO & Keyword Research API",
    "version": "1.0.0",
    "description": "VebAPI provides SEO and keyword data such as search volume, CPC, competition, and SERP results."
  },
  "servers": [
    {
      "url": "https://vebapi.com/api",
      "description": "Production API server"
    }
  ],
  "tags": [
    {
      "name": "Keyword Research",
      "description": "Keyword research and analysis APIs"
    },
    {
      "name": "On Page SEO",
      "description": "On-page SEO analysis and website structure checks"
    },
    {
      "name": "Youtube APIs",
      "description": "Youtube APIs"
    },
    {
      "name": "Backlink Analysis",
      "description": "Backlink analysis and backlink data"
    },
    {
      "name": "Domain Intelligence",
      "description": "Domain age, WHOIS and registration information"
    },
    {
      "name": "Website Screenshots",
      "description": "Website screenshots for SEO reports, compliance verification, monitoring, and archiving"
    },
    {
      "name": "VebAPI Insights",
      "description": "Manage Your VebAPI Account"
    }
  ],
  "paths": {
    "/seo/keywordresearch": {
      "get": {
        "tags": [
          "Keyword Research"
        ],
        "summary": "Keyword Research",
        "description": "Fetch keyword ideas, volume, CPC, and competition data for a given keyword and country.",
        "parameters": [
          {
            "name": "keyword",
            "in": "query",
            "description": "Keyword or phrase to analyze",
            "required": true,
            "schema": {
              "type": "string",
              "example": "email marketing"
            }
          },
          {
            "name": "country",
            "in": "query",
            "description": "Country code (e.g., us, in, uk)",
            "required": true,
            "schema": {
              "type": "string",
              "example": "us"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "example": [
                  {
                    "text": "email marketing tools",
                    "cpc": "8.40",
                    "vol": 4655,
                    "competition": "Medium",
                    "score": "1.68"
                  }
                ]
              }
            }
          },
          "401": {
            "description": "Invalid or missing API key"
          },
          "429": {
            "description": "Rate limit exceeded"
          }
        },
        "security": [
          {
            "ApiKeyAuth": []
          }
        ]
      }
    },
