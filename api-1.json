    "/seo/aiseochecker": {
      "get": {
        "tags": [
          "On Page SEO"
        ],
        "summary": "AI SEO Crawler Check",
        "description": "Check whether a website allows AI bots (e.g., GPTBot, Google-Extended, PerplexityBot) based on robots.txt rules.",
        "parameters": [
          {
            "name": "website",
            "in": "query",
            "description": "Website domain or full URL to check",
            "required": true,
            "schema": {
              "type": "string",
              "example": "codeconia.com"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "example": {
                  "url": "codeconia.com",
                  "robots_found": true,
                  "ai_access": {
                    "GPTBot": true,
                    "ChatGPT-User": true,
                    "Google-Extended": true,
                    "AnthropicBot": true,
                    "ClaudeBot": true,
                    "PerplexityBot": true
                  },
                  "ai_bots_allowed": true,
                  "suggestions": [
                    "Your site is currently open for AI bots. You're AI-friendly!",
                    "You can still improve by providing structured data (schema.org) for better AI comprehension.",
                    "Add clear content usage terms if you want to allow or limit AI training use."
                  ]
                }
              }
            }
          },
          "401": {
            "description": "Invalid or missing API key"
          },
          "429": {
            "description": "Rate limit exceeded"
          }
        },
        "security": [
          {
            "ApiKeyAuth": []
          }
        ]
      }
    },
